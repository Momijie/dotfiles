alias archw='librewolf --new-tab wiki.archlinux.org &'
alias cls='clear'
alias status='systemctl status'
alias sz='source ~/.zshrc'
alias ls='ls --color=auto --time-style=+%D\ %I:%m\ %p'
alias py='python3'
alias splex='systemctl start plexmediaserver.service'
alias osteam='cd ~/.local/share/Steam/steamapps/common'
alias sudo='doas'
alias dotfiles='cd ~/dotfiles'
alias archinfo='nvim ~/Documents/arch/arch_information'
alias build='mkdir -p build/ ; cd build/ ; cmake .. ; make ; cd ..'
alias update='doas pacman -Syu'
alias code='vscodium'
